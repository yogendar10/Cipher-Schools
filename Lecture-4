
#include <iostream>
using namespace std;

int main() {
    // Declare two integer variables a and b
    int a = 3;
    int b = 5;

    // Calculate the sum of a and b and print the result
    cout << "The sum of " << a << " and " << b << " is: " << a + b << endl;

    // Print the expression "a+b"
    cout << "Expression: a+b" << endl;

    return 0;
}
